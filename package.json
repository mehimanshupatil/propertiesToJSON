{
    "name": "js-properties-to-json",
    "version": "0.0.1",
    "description": "Convert a Java properties file to JSON (with JavaScript).",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist/"
    ],
    "scripts": {
        "serve": "serve ./",
        "build": "tsc",
        "test": "node ./test/test.js",
        "watch": "onchange \"src/**/*.*\" -v -- npm run buildTest",
        "buildTest": "npm run build && npm run test",
        "deploy": "gh-pages -d ./"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mehimanshupatil/propertiesToJSON.git"
    },
    "keywords": [
        "java",
        "properties",
        "json",
        "conversion"
    ],
    "author": "Himanshu Patil",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/mehimanshupatil/propertiesToJSON/issues"
    },
    "homepage": "https://github.com/mehimanshupatil/propertiesToJSON#readme",
    "devDependencies": {
        "@types/node": "^17.0.23",
        "gh-pages": "^3.2.3",
        "onchange": "^7.1.0",
        "serve": "^13.0.2",
        "tslib": "^2.3.1",
        "typescript": "^4.4.4"
    }
}
