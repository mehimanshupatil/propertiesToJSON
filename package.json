{
    "name": "properties-to-json",
    "version": "0.0.1",
    "description": "Convert a Java properties file to JSON (with JavaScript).",
    "main": "./dist/index.js",
    "module": "./dist/index.es.js",
    "source": "./src/index.ts",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/"
    ],
    "scripts": {
        "serve": "serve ./",
        "rollup": "rollup --config",
        "test": "node test.js",
        "watch": "onchange \"src/**/*.*\" -v -- npm run buildTest",
        "buildTest": "npm run rollup && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mehimanshupatil/propertiesToJSON.git"
    },
    "keywords": [
        "java",
        "properties",
        "json",
        "conversion"
    ],
    "author": "Himanshu Patil",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/mehimanshupatil/propertiesToJSON/issues"
    },
    "homepage": "https://github.com/mehimanshupatil/propertiesToJSON#readme",
    "devDependencies": {
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-typescript": "^8.3.0",
        "@types/node": "^16.11.6",
        "onchange": "^7.1.0",
        "rollup": "^2.58.3",
        "serve": "^12.0.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.4"
    }
